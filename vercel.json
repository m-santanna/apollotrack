{
    "version": 2,

    "buildCommand": "bun run build",
    "outputDirectory": ".output",

    "functions": {
        ".output/server/index.mjs": {
            "runtime": "nodejs20.x"
        }
    },

    "routes": [
        {
            "src": "/(.*)",
            "dest": ".output/server/index.mjs"
        }
    ]
}
